function start() {
    putBall();
    for(var i = 0; i < 5;i++ ){
        move();
    }
    putBall();
    turnLeft();
    for(var i = 0; i < 5;i++ ){
        move();
    }
    putBall();
    turnLeft();
    for(var i = 0; i < 5;i++ ){
        move();
    }
    putBall();
    turnLeft();
    for(var i = 0; i < 5;i++ ){
        move();
    }
    turnLeft();
}
